<div class="row">
    <div class="col-md-12">        
            <div class="row">
                <div class="col-md-12">
                    <div id="checkboxes">
                        @foreach (var item in ViewBag.Grade)
                        {
                            <div class="checkbox checkbox-warning">
                                <input class="styled" id=@item.GradeId type="checkbox" />
                                <label for=@item.GradeId>@item.GradeDescription</label>
                            </div>
                        }
                    </div>
                </div>
            </div>        
    </div>
</div>
