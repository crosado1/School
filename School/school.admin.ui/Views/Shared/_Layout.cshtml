<body>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src=@Url.Content("~/Scripts/template/jquery.js")></script>

    <!-- jQuery UI JS -->
    <script src=@Url.Content("~/Scripts/template/jquery-ui-v1.10.3.js")></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src=@Url.Content("~/Scripts/template/bootstrap.min.js")></script>
    <!-- Sparkline graphs -->
    <script src=@Url.Content("~/Scripts/template/sparkline.js")></script>
    <!-- jquery ScrollUp JS -->
    <script src=@Url.Content("~/Scripts/template/scrollup//jquery.scrollUp.js")></script>
    <!-- Notifications JS -->
    <script src=@Url.Content("~/Scripts/template/alertify/alertify.js")></script>
    <script src=@Url.Content("~/Scripts/template/alertify/alertify-custom.js")></script>
    <!-- Flot Charts -->
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.tooltip.min.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.resize.min.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.stack.min.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.orderBar.min.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/jquery.flot.pie.min.js")></script>
    <!-- JVector Map -->
    <script src=@Url.Content("~/Scripts/template/jvectormap/jquery-jvectormap-1.2.2.min.js")></script>
    <script src=@Url.Content("~/Scripts/template/jvectormap/jquery-jvectormap-usa.js")></script>


    <!-- Custom JS -->
    <script src=@Url.Content("~/Scripts/template/flot/custom/line.js")></script>

    <script src=@Url.Content("~/Scripts/template/flot/custom/area.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/combine-chart.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/combine-chart-compare.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/stacked-area.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/vertical.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/horizontal.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/stacked-vertical.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/stacked-horizontal.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/rectangular-pie.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/realtime.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/pie.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/donut.js")></script>
    <script src=@Url.Content("~/Scripts/template/flot/custom/scatter.js")></script>   
   

    <!-- Custom Index -->
    <script src=@Url.Content("~/Scripts/template/custom.js")></script>
    <script src=@Url.Content("~/Scripts/template/custom-index.js")></script>

    <!-- Header Start -->
    <header>
        <!-- Logo starts -->
        <div class="logo">
            <a href="#">
                <img src=@Url.Content("~/img/logo.png") alt="logo">
                <span class="menu-toggle hidden-xs">
                    <i class="fa fa-bars"></i>
                </span>
            </a>
        </div>
        <!-- Logo ends -->
        <!-- Mini right nav starts -->
        <div class="pull-right">
            <ul id="mini-nav" class="clearfix">
                <li class="list-box hidden-lg hidden-md hidden-sm" id="mob-nav">
                    <a href="#">
                        <i class="fa fa-reorder"></i>
                    </a>
                </li>
                <li class="list-box user-profile hidden-xs">
                    <a href="login.html" class="user-avtar animated rubberBand">
                        <img src=@Url.Content("~/img/user4.jpg") alt="user avatar">
                    </a>
                </li>
            </ul>
        </div>
        <!-- Mini right nav ends -->
    </header>
    <!-- Header ends -->

    <!-- Left sidebar starts -->
    <aside id="sidebar">
