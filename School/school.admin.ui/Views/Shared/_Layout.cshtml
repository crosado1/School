<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Everest Admin Panel" />
    <meta name="keywords" content="Admin, Dashboard, Bootstrap3, Sass, transform, CSS3, HTML5, Web design, UI Design, Responsive Dashboard, Responsive Admin, Admin Theme, Best Admin UI, Bootstrap Theme, Wrapbootstrap, Bootstrap" />
    <meta name="author" content="Bootstrap Gallery" />
    <link rel="shortcut icon" href="img/favicon.ico">


    <title>@ViewBag.Title - My ASP.NET Application</title>

    <!-- Bootstrap CSS -->
    <link href=@Url.Content("~/Content/template/css/bootstrap.css") rel="stylesheet" media="screen" />
    <!-- Animate CSS -->
    <link href=@Url.Content("~/Content/template/css/animate.css") rel="stylesheet" media="screen" />
    <!-- Alertify CSS -->
    <link href=@Url.Content("~/Content/template/css/alertify/alertify.core.css") rel="stylesheet" />
    <link href=@Url.Content("~/Content/template/css/alertify/alertify.default.css") rel="stylesheet" />
    <!-- Main CSS
    <link href="css/main.css" rel="stylesheet" media="screen">-->
    <link href=@Url.Content("~/Content/template/css/main.css") rel="stylesheet" />
    <!-- Datepicker CSS -->
    <link href=@Url.Content("~/Content/template/css/datepicker.css") rel="stylesheet" />
    <!-- Font Awesome -->
    <link href=@Url.Content("~/fonts/font-awesome.min.css") rel="stylesheet" />
    <!-- HTML5 shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="js/html5shiv.js"></script>
        <script src="js/respond.min.js"></script>
    <![endif]-->


    @*@Styles.Render("~/Content/css")
